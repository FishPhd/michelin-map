{
  "name": "turborepo-basic-yarn",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "api/*",
    "packages/*",
    "scraper/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "next": "^12.1.0",
    "prettier": "^2.5.1",
    "turbo": "^1.1.3"
  }
}
