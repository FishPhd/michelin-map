{
  "name": "api",
  "version": "1.0.0",
  "main": "index.js",
  "packageManager": "yarn@1.22.17",
  "scripts": {
    "start": "node dist/index.js",
    "watch": "tsc -w",
    "dev": "tsnd --respawn dist/index.js",
    "migrate": "prisma migrate dev",
    "pull": "prisma db pull",
    "gen": "prisma generate && prettier --write prisma/**/*.ts",
    "lint": "eslint . --ext .ts --fix",
    "prettier": "prettier --write prisma/**/*.ts",
    "build": "tsc"
  },
  "license": "MIT",
  "dependencies": {
    "apollo-server": "^3.6.3",
    "class-validator": "^0.13.2",
    "graphql": "15.7.2",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.14.1",
    "prisma": "~3.10.0",
    "reflect-metadata": "^0.1.13",
    "typescript": "^4.5.5"
  },
  "devDependencies": {
    "@prisma/client": "~3.10.0",
    "@types/graphql-fields": "^1.3.4",
    "@types/node": "^17.0.19",
    "eslint-plugin-unused-imports": "^2.0.0",
    "ts-node": "^10.5.0",
    "ts-node-dev": "^1.1.8",
    "type-graphql": "^1.1.1",
    "typegraphql-prisma": "^0.19.1"
  },
  "engines": {
    "yarn": "1.x"
  }
}
